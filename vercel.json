{
  "rewrites": [{
    "source": "/app",
    "destination": "/index.html"
  },
    {
      "source": "/app/(.*)",
      "destination": "/index.html"
    }],
  "redirects": [{
    "source": "/",
    "has": [{
      "type": "cookie",
      "key": "snipai_user"
    }],
    "destination": "/app",
    "permanent": false
  }],
  "headers": [{
    "source": "/sw.js",
    "headers": [{
      "key": "Content-Type",
      "value": "application/javascript"
    },
      {
        "key": "Service-Worker-Allowed",
        "value": "/"
      },
      {
        "key": "Cache-Control",
        "value": "no-cache"
      }]
  },
    {
      "source": "/manifest.json",
      "headers": [{
        "key": "Content-Type",
        "value": "application/manifest+json"
      }]
    },
    {
      "source": "/(.*)",
      "headers": [{
        "key": "X-Content-Type-Options",
        "value": "nosniff"
      },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }]
    }]
}